<h1>Listing locals</h1>

<table>
  <tr>
    <th>Name</th>
    <th>Code</th>
    <th>Union</th>
    <th>Porcent</th>
    <th>Expensas</th>
    <th>Shopping</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @locals.each do |local| %>
  <tr>
    <td><%= local.name %></td>
    <td><%= local.code %></td>
    <td><%= local.union %></td>
    <td><%= local.porcent %></td>
    <td><%= local.expensas %></td>
    <td><%= local.shopping %></td>
    <td><%= link_to 'Show', [@shopping, local] %></td>
    <td><%= link_to 'Edit', edit_shopping_local_path(@shopping, local) %></td>
    <td><%= link_to 'Destroy', [@shopping, local], :confirm => 'Are you sure?', :method => :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Local', new_shopping_local_path(@shopping) %>
